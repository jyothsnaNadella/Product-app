@model IEnumerable<ProductsProject.Models.Products>

@using ProductsProject.Models;

@{
    ViewBag.Title = "Product List";
}
    <div>
        <h2>Product List</h2>
        <ul>
            @foreach(Products product in @Model)
            {
                <li>
                    @Html.ActionLink(product.ProductName,"Products",new { @id = product.ProductId });
                </li>
            }
        </ul>
    </div>